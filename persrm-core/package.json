{
  "name": "persrm-core",
  "version": "0.1.0",
  "description": "Core reasoning model for PersRM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "cli": "node bin/cli-wrapper.js",
